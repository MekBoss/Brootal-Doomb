		damagefactor "Crush", 50.0;
		damagefactor "Blood", 0.0;
		damagefactor "Trample", 0.0;
		//+NORADIUSDMG
		+NOPAIN;
		+CORPSE;
		
		Mass 6000;
		damagefactor "Crush", 50.0;
		damagefactor "Blood", 0.0;
		damagefactor "Trample", 0.0;
		//+NORADIUSDMG
		+NOPAIN;
		+CORPSE;
		
		
		+NOPAIN;
		Health 200;
		DamageFactor "Head", 15.0;
		Height 22;
		Radius 6;
		Scale 0.9;
	}
	States
	{
		Spawn:
			TNT1 A 0;
			TNT1 A 0 A_Recoil(6);
			BR95 A 6;
			TNT1 A 0 A_JumpIf(vel.x < 0, "LayDown");
			TNT1 A 0 A_JumpIf(vel.y < 0, "LayDown");
