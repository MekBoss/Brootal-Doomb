		Health 200;
	Height 22+15;
	doomb_monster_base.head_starting_position 22;
		Radius 6;
		Scale 0.9;
	}
	States
	{
		Spawn:
			TNT1 A 0;
			TNT1 A 0 A_Recoil(6);
