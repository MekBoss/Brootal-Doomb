CLASS BrutalizedCommando1 : doomb_body_base
{
Default
{
	Radius 16;
	Height 50;
	Health 300;
	damagefactor "Blood", 0.0;
	damagefactor "BlueBlood", 0.0;
	damagefactor "GreenBlood", 0.0;
	PainSound "grunt/pain";
	Mass 999999;
	-SOLID;
	+SHOOTABLE;
	+FLOORCLIP;
	+DONTSPLASH;
	+nopain
	Scale 1.0;
}
States
{
	Spawn:
		CPOD JJJJJJJJJJJJJJJJJJJJJJJJJJJJJJ 1
		{
			if(health <= 200)
				setstatelabel("no_hand");
			if(random(0, 10) == 10)
				doomb_for_khorne("TinyBlood", (0, 0, 23), (frandom(-5, 5), frandom(-5, 5), frandom(0, 5) ) );
		}
		CPOD K 6
		{
			A_SpawnItem ("MiniGun", 0, 5,0);
			A_SpawnItem ("BrutalizedDeadCommando1");
		}
	Stop;

	//BrutalizedCommando2	
	no_hand:
		TNT1 A 0 doomb_much_blood(0);
		TNT1 A 0 A_SpawnProjectile ("XDeathChainArm", 45, 0, random (0, 360), 2, random (0, 160));
		TNT1 A 0 A_SpawnProjectile ("XDeath1", 45, 0, random (0, 360), 2, random (0, 160));
		TNT1 A 0 A_PlaySound("chainguy/death");
		CPOD MNMNMNMNMNMNMNMNMNMNMNMNMNMNMNMNMNMN 1
		{
			if(health <= 100)
				setstatelabel("just_torso");
			if(random(0, 10) == 10)
				doomb_for_khorne("TinyBlood", (0, 0, 23), (frandom(-5, 5), frandom(-5, 5), frandom(0, 5) ) );
		}
		CPOD O 5
		{
			A_SpawnItem ("BrutalizedDeadChaingunGuy");
			A_SpawnItem ("MiniGun", 0, 5,0);
		}
	Stop;

	//BrutalizedCommando3	
	just_torso:
		TNT1 AAAA 0 A_SpawnProjectile ("Brains1", 50, 0, random (0, 360), 2, random (0, 160));
		TNT1 A 0 A_SpawnProjectile ("eye", 50, 0, random (0, 360), 2, random (0, 160));
		TNT1 A 0 A_SpawnProjectile ("head_piece_dark", 50, 0, random (0, 360), 2, random (0, 160));
		TNT1 A 0 A_SpawnProjectile ("jaw", 50, 0, random (0, 360), 2, random (0, 160));
		TNT1 AAAAAA 0 A_SpawnProjectile ("Brutal_Blood", 50, 0, random (0, 360), 2, random (0, 160));
		TNT1 AAAA 0 A_SpawnProjectile ("SmallBrainPiece", random (45, 55), random (5, -5), random (170, 190), 2, random (0, 40));
		TNT1 AAAA 0 A_SpawnProjectile ("SmallBrainPiece", 50, 0, random (0, 360), 2, random (0, 160));
		tnt1 a 0 doomb_spray_decal("BrutalBloodSuper", times: 5);
		TNT1 AA 0 A_SpawnProjectile ("XDeath1", 45, 0, random (0, 360), 2, random (0, 160));
		CPOD P 5;
		CPOD QQQQQQQQQQ 10 A_SpawnItem ("Blood", 0, 43);
		CPOD R 5;
		TNT1 A 0 A_SpawnItem ("BrutalizedDeadChaingunGuy");
	Stop;

	Death:
		TNT1 A 0 A_SpawnItem ("CrueltyBonus5Health");
		TNT1 AA 0 A_SpawnProjectile ("XDeath1", 45, 0, random (0, 360), 2, random (0, 160));
		TNT1 A 0 A_SpawnProjectile ("XDeath2", 45, 0, random (0, 360), 2, random (0, 160));
		TNT1 A 0 A_SpawnProjectile ("XDeath3", 45, 0, random (0, 360), 2, random (0, 160));
		TNT1 A 0 A_SpawnItem ("MiniGun", 0, 5,0);
		CPOD S -1 doomb_final_absolution(12, 12);
	Stop;
}

}


