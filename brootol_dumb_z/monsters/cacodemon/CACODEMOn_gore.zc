CLASS caco_with_head_hole : doomb_body_base
{ 
Default {
	Radius 24;
	Height 48;
	Health 250;
	damagefactor "Blood", 0.0;
	damagefactor "BlueBlood", 0.0;
	damagefactor "GreenBlood", 0.0;
	
	damagefactor "Cutless", 0.6;
	bloodtype "Blue_Blood";
	bloodcolor "darkblue";
	Mass 999999;
	-SOLID;
	+NOBLOODDECALS;
	+SHOOTABLE;
	+FLOORCLIP;
	+DONTSPLASH;
	+NOPAIN;
	Scale 1.0;
}
States
{
	Spawn:
		TNT1 A 0;
		TNT1 A 0
		{
			if(random(0, 10) < 2)
				a_die("no_suffer");
		}
		CCD1 AAEEAAEEAAAAEEAAEEAA 5
		{
			actor a = doomb_for_khorne("doomb_red_blood_trail", (0, 0, 30), (frandom(-7, 7), frandom(-7, 7), frandom(0, 7)) );
			if(a)
				a.a_settranslation("red_to_blue");

			if(health <= 120)
				setstatelabel("caco_no_head");
		}
		tnt1 a 0 a_die("head_hole");
	death.head_hole:
		CCD1 ABC 6;
		CCD1 C -1 doomb_final_absolution(12, 24);
	Stop;
		
	death.no_suffer:
		TNT1 a 0 A_NoBlocking;
		TNT1 A 0 A_SpawnProjectile ("CacoXDeath", 50, 0, random (0, 360), 2, random (20, 40));
		CCD2 B -1 doomb_final_absolution(12, 24);
	Stop;

	caco_no_head:
		TNT1 A 0 A_SpawnProjectile ("CacoXDeath", 50, 0, random (0, 360), 2, random (20, 40));
		CCD2 AAEEAAEEAAAAEEAAEEAA 5
		{
			actor a = doomb_for_khorne("doomb_red_blood_trail", (0, 0, 30), (frandom(-7, 7), frandom(-7, 7), frandom(0, 7)) );
			if(a)
				a.a_settranslation("red_to_blue");
		}
		tnt1 a 0 a_die("wound_no_head");
	death.wound_no_head:
		CCD2 B -1 doomb_final_absolution(12, 24);
	Stop;
		
	Death:
		TNT1 AAAAA 0 A_SpawnProjectile ("Blue_FlyingBlood", 47, 0, random (0, 360), 2, random (30, 150));
		TNT1 AAAA 0 A_SpawnProjectile ("Blue_FlyingBloodTrail", 0, 0, random (0, 360), 2, random (0, 180));
		TNT1 AA 0 A_SpawnProjectile ("CacoXDeath6", 30, 0, random (0, 360), 2, random (0, 160));
		TNT1 AAAAA 0 A_SpawnProjectile ("CacoXDeath7", 30, 0, random (0, 360), 2, random (0, 160));
		TNT1 A 0 A_SpawnItem ("CrueltyBonus5Health");
		CCD3 A 10;
		CCD3 B -1 doomb_final_absolution(12, 24);
	Stop;

}
	
}
