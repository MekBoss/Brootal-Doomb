
CLASS CacoXdeath1 : doomb_body_base
{ 
Default
{
	translation "168:191=192:207";
	Radius 8;
	Height 8;
	Speed 9;
	Scale 1.1;
	Mass 1;
	+NOBLOCKMAP;
	+MISSILE;
	+NOTELEPORT;
	+MOVEWITHSECTOR;
	+CLIENTSIDEONLY;
	+RIPPER;
	+BLOODLESSIMPACT;
	damagetype "Blood";
	SeeSound "misc/xdeath4";
	DeathSound "misc/xdeath1";
	Decal "BlueBloodSplat";
}

States
{
	Spawn:
		TNT1 AAAAAAAAAA 0 A_SpawnProjectile ("Blue_FlyingBloodTrail", 0, 0, random (0, 180), 2, random (0, 180));
		TNT1 A 0 A_JumpIf(waterlevel > 1, "null");
		CCP1 A 4
		{
			actor a = doomb_for_khorne("doomb_red_blood_trail");
			if(a)
				a.a_settranslation("red_to_blue");
		}
	Loop;

	Death:
		TNT1 AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA 0 A_SpawnProjectile ("Blue_FlyingBloodTrail", 0, 0, random (0, 180), 2, random (0, 180));
		TNT1 A 0 A_SpawnItem("Blue_BloodSpot",0,0,0,1);
		CCP1 B 3500
		{
			frame += random(0, 1);
		}
	stop;
}
}


