CLASS DyingZombieman : doomb_body_base
{ 
	Default {
	
	Radius 12;
	Height 16;
	Speed 9;
	Health 50;
	PainSound "grunt/pain";
	DeathSound "grunt/death";
	damagefactor "Kick", 6.0;
	PainChance "Kick", 255;
	damagefactor "trample", 2.0;
	damagefactor "Death.KickDowned", 100.0;
	damagefactor "Death.KickDownedAny", 100.0;
	+SHOOTABLE;
	+FLOORCLIP;
	//+NOBLOCKMAP
	+LOOKALLAROUND;
	+FRIGHTENED;
	-SOLID;
	+DONTSPLASH;
	MONSTER;
	+FRIENDLY;//
	-COUNTKILL;
	+GHOST;
	+THRUGHOST;
	Mass 500;
	}
    States
    {
	
	Pain.Kick:
		TNT1 A 0 A_SpawnProjectile ("Teeth", 30, 0, random (0, 360), 2, random (0, 160));
		TNT1 A 0 A_SpawnProjectile ("Blood", 30, 0, random (0, 360), 2, random (0, 160));
		TNT1 A 0 A_SpawnProjectile ("TeethNoBounce", 30, 0, random (0, 360), 2, random (0, 160));
		TNT1 A 0 A_SpawnItem("CrueltyBonus1health");
		TNT1 A 0 A_Pain;
		TNT1 A 0 A_FaceTarget;
		ZMAK ABA 4;
		ZMAM A 20;
		Goto See;
		
		
	Death.Fatality:
		TNT1 A 0 A_NoBlocking;
		TNT1 A 0 A_GiveToTarget("HasZmanShield", 1);
		TNT1 A 0 A_GiveToTarget("GotMeatShield", 1);
		Stop;
		
	Death.Kick:
	Death.KickDowned:
	Death.punch:
		TNT1 A 0 A_NoBlocking;
		TNT1 A 0 A_GiveToTarget("GoFatality", 1);
		TNT1 A 0 A_GiveToTarget("IsCurbstompingZombieman", 1);
		Stop;
		
	Spawn:
		TNT1 A 0;
		TNT1 A 0 A_GiveInventory("IsCrawling", 1);
		ZMAM A 1 A_Look;
		Loop;
	See:
		ZMAM ABAB 10 A_Chase;
		ZMAM A 9 A_Pain;
		TNT1 A 0 A_Jump(12, "LastStand");
		TNT1 A 0 A_SpawnItemEx("Brutal_FlyingBlood", 0, 0, 10, 0, 0);
		
			tnt1 a 0 a_jumpif(random(0, 10) < 1, "death");
		Loop;
		
	Death:
		TNT1 A 0 A_NoBlocking;
		TNT1 A 0 A_Pain;
		POSH Z -1;//doomb_final_absolution(12, 12);
		Stop;
		
	LastStand:
		TNT1 A 0 A_NoBlocking;
		TNT1 A 0 A_Pain;
		TNT1 A 0 A_SpawnItem ("ZombiemanLastStand1");
		Stop;
		
	Death.Fire:
		TNT1 A 0 A_SpawnItem ("CrueltyBonus5Armor");
		TNT1 A 0 A_SpawnItem ("CrueltyBonus5Health");
		
	mini_fire:
		TNT1 A 0 A_Playsound("BurnZom");
		ZMAB AABB 5 light("BIGREDTORCH")
			{
				A_Explode(3,30, damagetype:"fire");
