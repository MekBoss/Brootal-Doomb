CLASS Gib_ZombieLeg2: LimbBase
{
States
{
		Spawn:
			LEG1 A 1900 doomb_spawn_bloodpool(BPS_SMALL);
		Loop;

		Death.plasma:
			TNT1 A 0 doomb_severed_limb("XDeathBurnedLeg", (0, 0, 32), doomb_random_velocity(5) );
		Stop;

		Death:    
			TNT1 AAAA 0 doomb_for_khorne("doomb_red_blood_trail", (0, 0, 32 ), doomb_random_velocity(5) );
			TNT1 A 0 doomb_severed_limb("GibZombieBoot", vel_dir:doomb_random_velocity(5) );
			LEG1 Z -1;
		stop;

		Death.Kick:
		Death.Punch:
			LEG1 AA 1 A_FaceTarget;
			TNT1 A 0 doomb_severed_limb("XDeathZombieLegKicked");
		Stop;

		Pain:
			TNT1 A 0 ThrustThingZ(0,20,0,1);
			LEG1 ABCDEFGH 2;
		Goto Spawn;

		Death.Explosive_Impact:
			TNT1 A 0 A_NoBlocking;
			TNT1 A 0 ThrustThingZ(0,30,0,1);
			LEG1 A 5;
			LEG1 ABCDEFGHABCDEFGHABCDEFGH 5 A_CheckFloor("Landz");
		Landz:
			LEG1 E 1 doomb_spawn_bloodpool(BPS_SMALL);
			LEG1 E -1;
		stop;
	}
	
}




