CLASS DyingZombieman : doomb_body_base
{ 
	Default {
	
	Radius 12;
	Height 16;
	Speed 9;
	Health 50;
	PainSound "grunt/pain";
	DeathSound "grunt/death";
	damagefactor "Kick", 6.0;
	PainChance "Kick", 255;
	damagefactor "trample", 2.0;
	damagefactor "Death.KickDowned", 100.0;
	damagefactor "Death.KickDownedAny", 100.0;
	+SHOOTABLE;
	+FLOORCLIP;
	//+NOBLOCKMAP
	+LOOKALLAROUND;
	+FRIGHTENED;
	-SOLID;
	+DONTSPLASH;
	MONSTER;
	+FRIENDLY;//
	-COUNTKILL;
	+GHOST;
	+THRUGHOST;
	Mass 500;
	}
    States
    {
	
	Pain.Kick:
		TNT1 A 0 doomb_severed_limb("Teeth", (0, 0, 50), doomb_random_velocity(5) );
		TNT1 A 0 doomb_for_khorne("brutal_Blood", (0, 0, 30), doomb_random_velocity(5));
		TNT1 A 0 A_Givetotarget("CrueltyBonus1health");
		TNT1 A 0 A_Pain;
		TNT1 A 0 A_FaceTarget;
		ZMAK ABA 4;
		ZMAM A 20;
		Goto See;
		
		
	Death.Fatality:
		TNT1 A 0 A_NoBlocking;
		TNT1 A 0 A_GiveToTarget("HasZmanShield", 1);
		TNT1 A 0 A_GiveToTarget("GotMeatShield", 1);
	Stop;

	Death.Kick:
	Death.KickDowned:
	Death.punch:
		tnt1 A 0
		{
			doomb_monster_fatality_camera(target);
			A_Pain();
			A_NoBlocking();
		}
		01CB A 3;

		tnt1 A 0 A_PlaySound("grunt/death");
		01CB BCCCC 3 SetPlayerProperty(0,1,0);
		tnt1 A 0 A_PlaySound("misc/xdeath1");
		01CB D 3;
		tnt1 a 0 Radius_Quake (4, 24, 0, 15, 0);
		tnt1 A 0 ACS_Execute(580, 0, 0, 0, 0);
		tnt1 A 0 ACS_Execute(581, 0, 0, 0, 0);
		tnt1 A 0 ACS_Execute(582, 0, 0, 0, 0);
		tnt1 A 0 ACS_Execute(583, 0, 0, 0, 0);
		//spawn height +20
		TNT1 A 0 A_PlaySound("misc/xdeath4");
		TNT1 AAAAA 0 doomb_for_khorne("blood_impact_splash");
		TNT1 AAAAA 0 doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), doomb_random_velocity(5) );
		TNT1 A 0 doomb_severed_limb("eye", (0, 0, 50), doomb_random_velocity(5) );
		TNT1 A 0 doomb_severed_limb("head_piece", (0, 0, 50), doomb_random_velocity(5) );
		TNT1 A 0 doomb_severed_limb("jaw", (0, 0, 50), doomb_random_velocity(5) );
		TNT1 A 0 doomb_severed_limb("SmallBrainPiece", (0, 0, 50), doomb_random_velocity(5) );
		TNT1 A 0 doomb_severed_limb("Teeth", (0, 0, 50), doomb_random_velocity(5) );
		TNT1 a 0 doomb_spray_decal("BrutalBloodSuper", times: 5);
		TNT1 AA 0 doomb_spawn_entrails( (0, 0, random(30, 35) ) );
	
		TNT1 A 0 doomb_spawn_bloodpool();
		01CB E 20;
		tnt1 a 0
		{
			A_Givetotarget("HealthBonus",15);
			doomb_severed_limb("DeadZombieman_NoHeadfront");
			A_Givetotarget("HealthBonus",15);
			doomb_destroy_monster_fatality_camera();
		}
	Stop;
		
	Spawn:
		ZMAM A 1 A_Look;
		Loop;
	See:
		ZMAM ABAB 10 A_Chase;
		ZMAM A 9 A_Pain;
		TNT1 A 0 A_Jump(12, "LastStand");
		TNT1 A 0 doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), doomb_random_velocity(5) );
		
		tnt1 a 0 doomb_random_death();
	Loop;
		
	Death:
		TNT1 A 0 A_NoBlocking;
		TNT1 A 0 A_Pain;
		POSH Z -1;//doomb_final_absolution(12, 12);
	Stop;
		
	LastStand:
		TNT1 A 0 A_NoBlocking;
		TNT1 A 0 A_Pain;
		TNT1 A 0 doomb_severed_limb ("ZombiemanLastStand1");
	Stop;
		
	Death.Fire:
		TNT1 A 0 A_Givetotarget("CrueltyBonus5Armor");
		TNT1 A 0 A_Givetotarget("CrueltyBonus5Health");
		
	mini_fire:
		TNT1 A 0 A_Playsound("BurnZom");
		ZMAB AABB 5 light("BIGREDTORCH")
			{
				A_Explode(3,30, damagetype:"fire");
				{doomb_flare_base.doomb_attach_flare(self, "RedFlare");}
				doomb_show_effect("ExplosionSmoke", (0, 0, 20), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0.5, 1) ), (0.5, 0), 0.1 );
				doomb_show_effect("SmallFireTrail", velocity:(frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0, 2) ) );
				A_Chase();
				doomb_show_effect("ExplosionParticle2", (0, 0, 40), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0.5, 1) ), (frandom(0.01, 0.04), 0) );
			}
		TNT1 A 0 doomb_show_effect("SmallFireTrail", velocity:(frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0, 2) ) );
		TNT1 A 0 doomb_spawn_bloodpool(BPS_SMALL);
		tnt1 a 0 a_jumpif(random(0, 10) < 1, "death_fire");
	Loop;
		
	death_fire:
		TNT1 A 0 A_NoBlocking;
		TNT1 A 0 A_Scream;
		BRZ3 HHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHH 3 light("BIGREDTORCH")
		{
			A_Explode(3,30, damagetype:"fire");
			{doomb_flare_base.doomb_attach_flare(self, "RedFlare");}
			doomb_show_effect("ExplosionSmoke", (0, 0, 20), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0.5, 1) ), (0.5, 0), 0.1 );
			doomb_show_effect("SmallFireTrail", velocity:(frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0, 2) ) );
		}
		TNT1 A 0 doomb_show_effect("SmallFireFocus24", (frandom(-0.5, 0.5), frandom(-0.5, 0.5), 0 ) );
		BRZ3 HHHHHHHHHHHHHHHHHHHHHHHH 10 doomb_show_effect("ExplosionSmoke", (0, 0, 20), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0.5, 1) ), (0.5, 0), 0.1 );
		BRZ3 H -1;
		Stop;
		
	Death.Trample:
		TNT1 A 0 doomb_much_blood(10);
		TNT1 A 0 doomb_severed_limb("eye", (0, 0, 50), doomb_random_velocity(5) );
		TNT1 A 0 doomb_severed_limb("head_piece", (0, 0, 50), doomb_random_velocity(5) );
		TNT1 A 0 doomb_severed_limb("jaw", (0, 0, 50), doomb_random_velocity(5) );
		TNT1 A 0 doomb_severed_limb("SmallBrainPiece", (0, 0, 50), doomb_random_velocity(5) );
		TNT1 A 0 A_Givetotarget("CrueltyBonus5Armor");
		TNT1 AAAA 0 doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), doomb_random_velocity(5) );
		01CB F -1;//doomb_final_absolution(12, 12);
		Stop;
		Stop;
		
	Death.SSG:
	XDeath:
		TNT1 A 0 doomb_gore_spawner(null, (0, 0, 5), 6);
		TNT1 AAAAAA 0 bright doomb_spawn_entrails( (0, 0, random(30, 35) ) );
		TNT1 AA 0 bright doomb_spawn_entrails( (0, 0, random(30, 35) ) );
		TNT1 AA 0 bright doomb_spawn_entrails( (0, 0, random(30, 35) ) );
		TNT1 A 0 doomb_severed_limb("CrushedRemains");
		TNT1 A 1;
		TNT1 A 1 A_XScream;
		TNT1 A 1 A_NoBlocking;
		TNT1 A 0 A_Givetotarget("CrueltyBonus5Health");
	Stop;
		
	Death.Shotgun:
		TNT1 A 0 A_PlaySound("misc/xdeath4");
		TNT1 A 0 doomb_much_blood(20);

		TNT1 A 0 doomb_severed_limb("eye", (0, 0, 50), doomb_random_velocity(5) );
		TNT1 A 0 doomb_severed_limb("head_piece", (0, 0, 50), doomb_random_velocity(5) );
		TNT1 A 0 doomb_severed_limb("jaw", (0, 0, 50), doomb_random_velocity(5) );
		TNT1 A 0 doomb_severed_limb("SmallBrainPiece", (0, 0, 50), doomb_random_velocity(5) );
		
		ZMAD B 0 A_XScream;
		TNT1 A 0 A_Givetotarget("CrueltyBonus5Health");
		ZMAD C 0 A_NoBlocking;
		TNT1 A 0 doomb_severed_limb("DeadZombieman_Noheadfront");
	Stop;
		
	Crush:
	Death.Stomp:
		TNT1 AAAAAAAAAAAAAAAAAAAAAAAAA 0 doomb_for_khorne("doomb_red_blood_trail", (0, 0, 32 ), doomb_random_velocity(5) );
		TNT1 A 0 doomb_gore_spawner(null, (0, 0, 5), 6);
		TNT1 AAAAAA 0 bright doomb_spawn_entrails( (0, 0, random(30, 35) ) );
		TNT1 AA 0 bright doomb_spawn_entrails( (0, 0, random(30, 35) ) );
		TNT1 AA 0 bright doomb_spawn_entrails( (0, 0, random(30, 35) ) );
		TNT1 A 0 doomb_spawn_bloodpool();
		TNT1 A 0 doomb_severed_limb("CrushedRemains");
		TNT1 A 1;
		TNT1 A 1 A_XScream;
		TNT1 A 1 A_NoBlocking;
	Stop;
