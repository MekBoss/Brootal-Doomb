CLASS BurningZombie : doomb_body_base { 
	Default {
		
		Radius 12;
		Height 56;
		Speed 5;
		Scale 0.85;
		Health 100;
		GibHealth 25;
		damagefactor "Fire", 0.0;
		damagefactor "Explosive_Impact", 10.0;
		MONSTER;
		-COUNTKILL;
		+NOICEDEATH;
		+FRIENDLY;
		+FLOORCLIP;
		+SHOOTABLE;
		+NOBLOCKMONST;
		+DONTSPLASH;
		
		Gravity 0.7;
		bloodtype "DeadBlood";
		Obituary "%o was near someone in flames.";
		MaxDropOffHeight 6000;
		
		Mass 999999;
	}
	States
	{
		Spawn:
			tnt1 B 0 light("BIGREDTORCH")
			{
				A_Explode(3,30, damagetype:"fire");
				{Spawn("RedFlare", pos);}
				doomb_show_effect("ExplosionSmoke", (0, 0, 20), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0.5, 1) ), (0.5, 0), 0.1 );
				doomb_show_effect("SmallFireTrail", velocity:(frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0, 2) ) );
			}
			TNT1 A 0 A_PlaySound("BURNZOM");
		Suffer:
			BDT1 AAABBBCCCDDD 1 Bright light("BIGREDTORCH")
			{
				A_Wander();
				A_Explode(3,30, damagetype:"fire");
				{Spawn("RedFlare", pos);}
				doomb_show_effect("ExplosionSmoke", (0, 0, 20), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0.5, 1) ), (0.5, 0), 0.1 );
				doomb_show_effect("SmallFireTrail", velocity:(frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0, 2) ) );
				doomb_show_effect("ExplosionParticle2", (0, 0, 40), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0.5, 1) ), (frandom(0.01, 0.04), 0) );
			}

			TNT1 A 0 A_GiveInventory("PoorGuyBurningLife", 1);
			TNT1 A 0 A_JumpIfInventory("PoorGuyBurningLife", 12, "Death");
		Loop;
			
		Death.Explosive_Impact:
			TNT1 A 0;
			TNT1 A 0 ThrustThingZ(0,60,0,1);
			TNT1 A 0 A_SpawnItem ("CrueltyBonus5Health");
			TNT1 A 0;
		Looping:
			BDT3 AAAABBBBCCCCDDDD 1 Bright light("BIGREDTORCH")
			{
				A_Explode(3,30, damagetype:"fire");
				{Spawn("RedFlare", pos);}
				doomb_show_effect("ExplosionSmoke", (0, 0, 20), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0.5, 1) ), (0.5, 0), 0.1 );
				doomb_show_effect("SmallFireTrail", velocity:(frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0, 2) ) );
				if(vel.z == 0)
					setstatelabel("falldeath");
			}
			Loop;
			
		FallDeath:
			TNT1 AAAAAAAAAA 0 light("BIGREDTORCH")
			{
				A_Explode(3,30, damagetype:"fire");
				{Spawn("RedFlare", pos);}
				doomb_show_effect("ExplosionSmoke", (0, 0, 20), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0.5, 1) ), (0.5, 0), 0.1 );
				doomb_show_effect("SmallFireTrail", velocity:(frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0, 2) ) );
			}
			TNT1 AAAAAAAAAAAAAAAAAAAA 0 doomb_show_effect("ExplosionParticle2", (0, 0, 10), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0.5, 1) ) );
			TNT1 AAAA 0 A_SpawnProjectile("XDeath1", 10, 0, random (0, 360), 2, random (20, 150));
			TNT1 A 0 A_PlaySound("misc/xdeath");
			TNT1 A 0 A_Explode(100, 128);
		Death:
			TNT1 A 0 A_Fall;
			TNT1 A 0 A_Jump(128, "Death2");
			BDT1 EEFFGGHHiijjkkLLmm 3 Bright light("BIGREDTORCH")
			{
				A_Explode(3,30, damagetype:"fire");
				{Spawn("RedFlare", pos);}
				doomb_show_effect("ExplosionSmoke", (0, 0, 20), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0.5, 1) ), (0.5, 0), 0.1 );
				doomb_show_effect("SmallFireTrail", velocity:(frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0, 2) ) );
			}
			TNT1 A 0 doomb_show_effect("SmallFireFocus24", (frandom(-0.5, 0.5), frandom(-0.5, 0.5), 0 ) );
			NJEX AAAAAAAAAAAAAAAAAA 10 Bright doomb_show_effect("ExplosionSmoke", (0, 0, 20), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0.5, 1) ), (0.5, 0), 0.1 );
			NJEX A -1;
		Stop;
		
		Death2:
			BDT1 EEFFGGHHiijjkkLLmmBBBBBBBBBBBBBBBBBB 3 Bright light("BIGREDTORCH")
			{
				A_Explode(3,30, damagetype:"fire");
				{Spawn("RedFlare", pos);}
				doomb_show_effect("ExplosionSmoke", (0, 0, 20), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0.5, 1) ), (0.5, 0), 0.1 );
				doomb_show_effect("SmallFireTrail", velocity:(frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0, 2) ) );
			}
			TNT1 A 0 doomb_show_effect("SmallFireFocus24", (frandom(-0.5, 0.5), frandom(-0.5, 0.5), 0 ) );
			NJEX BBBBBBBBBBBBBBBBBB 10 Bright doomb_show_effect("ExplosionSmoke", (0, 0, 20), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0.5, 1) ), (0.5, 0), 0.1 );
			NJEX B -1;
		Stop;

		XDEATH:
		DEATH.SSG:
			TNT1 A 0 A_PlaySound("misc/xdeath");
			TNT1 AA 0 A_SpawnProjectile ("XDeathBurnedArm", 32, 0, random (0, 360), 2, random (0, 160));
			TNT1 AA 0 A_SpawnProjectile ("XDeathBurnedLeg", 32, 0, random (0, 360), 2, random (0, 160));
			TNT1 A 0 A_SpawnProjectile ("XDeathBurnedSkull", 32, 0, random (0, 360), 2, random (0, 160));
			TNT1 A 0 A_SpawnProjectile ("MuchMeatDeath", 35, 0, random (0, 360), 2, random (0, 160));
			TNT1 AAAAA 0 A_SpawnProjectile ("BurningFlyingMeatPiece", 64, 0, random (0, 360), 2, random (0, 160));
			TNT1 A 0 A_SpawnItem ("CrueltyBonus5Health");
			Stop;
	}
}


