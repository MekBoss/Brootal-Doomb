CLASS doomb_zombie_Man : doomb_monster_base
{
private uint ammo_amount;

Default {
	
	Health 50;
	Radius 16;
	Height 56;
	
	doomb_monster_base.headshot_multiplier 2;
	doomb_monster_base.head_starting_position 50;
	doomb_monster_base.legs_height_max 10;
	doomb_monster_base.weapon_type "rifle";

	Speed 6;
	Mass 300;
	FastSpeed 12;
	PainChance 200;
	PainChance "Kick", 255;
	PainChance "Melee", 255;
	PainChance "Taunt", 255;
	PainChance "HelperMarineFatality", 255;
	damagefactor "Crush", 5.0;
	damagefactor "HelperMarineFatallity", 3.0;
	damagefactor "SSG", 50.0;
	damagefactor "Explosive_Impact", 2.5;
	damagefactor "Taunt", 1.0;
	damagefactor "cut", 0.7;
	damagefactor "saw", 0.7;
	PainChance "Trample", 255;
	PainChance "Explosive_Impact", 255;
	damagetype "Melee";
	
	Monster;
	+FLOORCLIP;
	+RANDOMIZE;
	+SLIDESONWALLS;
	+DONTHARMCLASS
	+DONTHARMSPECIES
	
	Scale 0.98;
	MaxStepHeight 24;
	MaxDropOffHeight 32;
	BloodType "Brutal_Blood",  "SawBlood";
	SeeSound "grunt/sight";
	AttackSound "weapons/rifle";
	PainSound "grunt/pain";
	DeathSound "grunt/death";
	ActiveSound "grunt/active";
	damagefactor "Blood", 0.0;
	damagefactor "BlueBlood", 0.0;
	damagefactor "GreenBlood", 0.0;
	Obituary "%o has been gunned down by a Zombie Soldier.";
}
States
{
	Spawn:
		TNT1 A 0 nodelay {ammo_amount = 20;}
	Stand:
		PSSS ABABABABAB 10 A_Look;
	Loop;
		
	SearchForPlayer:
	Idle:
		TNT1 A 0 A_ClearTarget;
		TNT1 A 0 doomb_update_memory();
		PSSS ABACADA 10 A_Look;
		POSS ABCD 4
		{
			A_Look();
			A_Wander();
		}
	Loop;
		
		
	FallBack:
		TNT1 A 0 A_CheckSight("Advance");
		TNT1 A 0 {bNODROPOFF = true;}
		TNT1 A 0 A_jump(64, "Missile");
		POSS DDCCBBAA 2
		{
			A_FaceTarget();
			A_Recoil(4);
		}
		TNT1 A 0 {bNODROPOFF = false;}
	GOto Missile;
		
	See:
		TNT1 A 0 {bNODROPOFF = false;}
		TNT1 A 0 doomb_update_memory();
		TNT1 A 0 A_CheckSight("SeeNeverSeen");
	SeeContinue:
		TNT1 A 0 A_JumpIfCloser(200, "FallBack");
	Advance:
		TNT1 A 0 {bNODROPOFF = false;}
		POSS AABBCCDD 2 A_Chase;
		TNT1 A 0 A_JumpIfInTargetInventory("IsInvisible", 1, "Idle");
		TNT1 A 0 doomb_monster_memory();
	Loop;
		
	SeeNeverSeen:
	Advance:
		TNT1 A 0 {bNODROPOFF = false;}
		POSS AABBCCDD 2 A_Chase;
	Loop;
		
	Pain.Taunt:
		TNT1 A 0 A_JumpIfInventory("Enraged", 1, "Missile");
		POSS B 0 A_Pain;
		TNT1 A 0 {bMISSILEMORE = true;}
		TNT1 A 0 {bMISSILEEVENMORE = true;}
		TNT1 A 0 A_PlaySound("grunt/sight");
		POSS B 1 A_FaceTarget;
		POSS B 10;
		TNT1 A 0 A_GiveInventory("Enraged", 1);
		TNT1 A 0 A_PlaySound("grunt/sight", 1);
	Goto Missile;
		
	Missile:
		TNT1 A 0 doomb_update_memory();
		TNT1 A 0
		{
			if(ammo_amount <= 0)
			{
				setstatelabel("reload");
				return;
			}
		}
		POSS E 1 A_Stop;
		TNT1 A 0 {bNODROPOFF = false;}
		TNT1 A 0 A_Jump(80, "Grenade");
		
		POSS EE 3 A_FaceTarget;
		POSS EFEFEFE 2
		{
			if(frame == 5)
			{
				A_SpawnProjectile("ZombieManTracer", 32, 0, random(-8, 8), 1, random(-3, 3));
				A_SpawnItem ("RifleCaseSpawn", 0, 30,0);
				A_PlaySound("weapons/rifle");
				ammo_amount --;
			}
			A_FaceTarget();
			A_PlaySOund("DISTFRRIFLE");
			{doomb_flare_base.doomb_attach_flare(self, "lens_flare_orange", (0, 0, 36) );}
		}
		TNT1 A 0 A_jump(96, "Missile");
		
		POSS E 6 A_FaceTarget;
		TNT1 A 0
		{
			if(ammo_amount <= 0)
			{
				setstatelabel("reload");
				return;
			}
		}
		TNT1 A 0 A_JumpIfInTargetInventory("IsInvisible", 1, "SearchForPlayer");
	Goto SeeContinue;
		
		
	Grenade:
		TNT1 A 0 doomb_update_memory();
		TNT1 A 0 A_Stop;
		TNT1 A 0 {bNODROPOFF = false;}
		TNT1 A 0 A_JumpIfCloser(500, "Missile");
		POSG A 6 A_FaceTarget;
		TNT1 A 0 A_PLaySound ("GRNPIN");
		POSG A 6 A_FaceTarget;
		POSG B 6 A_SpawnProjectile("ZombiemanGrenade", 42);
	Goto SeeContinue;
		
	Reload:
		POSS E 3 A_SpawnProjectile("EmptyClipSpawn", 38, 0, random(-11, 11), 0);
		POSS E 1 A_PlaySound("Reload");
		ID12 A 30 {ammo_amount = 20;}
		ID12 C 10 A_PlaySound("CLIPIN");
		ID12 D 10;
		POSS A 10;
	Goto SeeContinue;
		
	//MARK V: Currently unused. Used to make the enemy rolls left or right when hit by a rocket alert actor.
	//Apeirogom: But I dont think so :v
	Avoid:
		POSS A 1 A_FaceTarget;
		TNT1 A 0 ThrustThing((int)(angle*256/360+randompick(64, 192)), 15, 0, 0);
		ZRO2 ABCDEFGH 3 A_FaceTarget;
	Goto Missile;
		
	Pain:
		TNT1 A 0 A_JumpIfInventory ("IsDown", 1, 4);
		POSS G 5;
		POSS G 5 A_Pain;
		TNT1 A 0 A_Jump(196, "Missile");
	Goto SeeContinue;
		ID11 F 6 A_Pain;
	Goto GetUp;
		
	Pain.punch:
		POSS G 4 A_JumpIfInventory ("IsDown", 1, 2);
		POSS G 4 A_Pain;
	Goto SeeContinue;
		TNT1 A 0 A_Pain;
		ID11 FRRRQ 10;
		TNT1 A 0 A_TakeInventory("IsDown", 1);
	Goto SeeContinue;
		
	Pain.Explosive_Impact:
		TNT1 A 0 {bNODROPOFF = false;}
		TNT1 A 0 A_Pain;
		TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Pain");
		TNT1 A 0 A_GiveInventory("IsDown", 1);
		TNT1 A 0 ThrustThingZ(0,40,0,1);
		POS4 UVWXY 5;
	Goto FallingAfterImpact;
		
	Pain.Kick:
	Pain.HeavyImpact:
		TNT1 A 0 {bNODROPOFF = false;}
		POS4 U 0 A_Pain;
		TNT1 A 0 {bFORCEXYBILLBOARD = true;}
		TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Pain");
		POS4 U 1 A_facetarget;
		TNT1 A 0 ThrustThingZ(0,30,0,1);
		TNT1 A 0 A_FaceTarget;
		POS4 U 3 ThrustThing((int)(angle*256/360+128), 10, 0, 0);
		TNT1 A 0 A_GiveInventory("IsDown", 1);
		POS4 UVWXY 3;
	FallingAfterImpact:
		ID11 F 1 A_GiveInventory ("FallingHeight", 1);
		ID11 F 1 A_CheckFloor ("GetUp");
		tnt1 A 0 A_JumpIf (vel.z == 0, "GetUp");
	Loop;
		
	GetUp:
		TNT1 A 0
		{
			if(waterlevel > 1)
				A_SpawnItem("LargeMassWaterImpact");
		}
		TNT1 A 0 A_JumpIfInventory ("FallingHeight", 5, "FallingDie");
		TNT1 A 0 A_TakeInventory("FallingHeight", 100);
		TNT1 A 0 A_Stop;
		ID11 RRRRRRRRRR 5 A_JumpIf(vel.z < 0, "FallingAfterImpact");
		TNT1 A 0 A_TakeInventory("IsDown", 1);
		ID11 Q 10;
		TNT1 A 0 {bFORCEXYBILLBOARD = false;}
	Goto SeeContinue;
		
	FallingDie:
		TNT1 A 0 A_ChangeCountFlags(false, false);
		TNT1 A 0 doomb_spawn_bloodpool();
		TNT1 A 0 A_JumpIfInventory ("FallingHeight", 15, "Death.Stomp");
		TNT1 A 0 A_JumpIfInventory ("FallingHeight", 10, "FallingSuperDeath");
		TNT1 A 0 A_JumpIfInventory ("FallingHeight", 5, "FallingDeath");
	FallingDeath:
		TNT1 A 0 A_Scream;
		TNT1 A 0 A_NoBLocking;
		TNT1 AAAA 0 doomb_spawn_entrails( (0, 0, random(30, 35) ) );
		ID11 EF 2;
		TNT1 A 0 doomb_spawn_bloodpool();
		TNT1 A 0 doomb_severed_limb("DeadZombieman1", limb_ignore_cvar: true);
	Stop;
		
	FallingSuperDeath:
		TNT1 A 0 A_XScream;
		TNT1 A 0 A_NoBLocking;
		TNT1 A 0 doomb_spawn_bloodpool();
	Goto Death.Explosive_Impact+1;

	Death.punch:
		TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Death.Down");
	Death.Kick:
		TNT1 A 0 {bNODROPOFF = false;}
		TNT1 A 0 {bSLIDESONWALLS = true;}
		TNT1 A 0 ThrustThingZ(0,30,0,1);
		TNT1 A 0 A_Screamandunblock;
		TNT1 A 0 A_Recoil (10);
		POS4 UUVWXY 5;
		TNT1 A 0 doomb_severed_limb("DeadZombieman1", limb_ignore_cvar: true);
	Stop;





	Death.Trample:
	Death.Down:
		TNT1 A 0 A_Screamandunblock;
		TNT1 A 0 A_Stop;
		TNT1 AA 0 doomb_spawn_entrails( (0, 0, random(30, 35) ) );
		ID11 F 12;
		POSS K 3;
		TNT1 A 0 doomb_severed_limb("DeadZombieman1", limb_ignore_cvar: true);
	Stop;
		
	Death.Shotgun:
		TNT1 A 0 A_Jump(42, "Death.Shotgunontheface");
		TNT1 A 0 A_JumpIfCloser(80, "Death.Blast");
		TNT1 A 0 A_JumpIfCloser(320, "Death.Shotgunontheface");
	Goto Death;
		
	Death.Shotgunontheface:
	Death.HeavyImpact:
		TNT1 A 0 {bSLIDESONWALLS = false;}
		TNT1 A 0 A_Jump(160, "DeathShotgunFly");
		POSS H 1 A_Screamandunblock;
		TNT1 A 0 A_Recoil(13);
		TNT1 A 0 ThrustThingZ(0,20,0,1);
		TNT1 A 0 A_JumpIf(vel != (0, 0, 0), "DeathShotgunFly");
		TNT1 A 0 A_Jump(128, "DeathShotgunontheface2");
		TNT1 A 0 A_Scream;
		TNT1 A 0 doomb_much_blood(35);
		TNT1 AA 0 doomb_spawn_entrails( (0, 0, random(30, 35) ) );
		TNT1 A 0 A_NoBlocking;
		POS4 ABCD 4 doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), doomb_random_velocity(5) );
		POS4 EFGHIJ 4 A_JumpIf((vel == (0, 0, 0) ), "DSFStop");
	Goto DSFStop+1;
		
	DSFStop:
		TNT1 A 0 A_Stop;
		TNT1 A 0 doomb_spawn_bloodpool();
		TNT1 a 0 doomb_spray_decal("BrutalBloodSuper", times: 5);
		TNT1 A 0 doomb_severed_limb("DeadZombiemanShotguntotheFace1", limb_ignore_cvar: true);
	Stop;
		
	DeathShotgunontheface2:
		TNT1 A 0 doomb_much_blood(35);
		TNT1 AA 0 doomb_spawn_entrails( (0, 0, random(30, 35) ) );
		POS4 KLMN 4 doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), doomb_random_velocity(5) );
		POS4 OPQRST 4 A_JumpIf((vel.y == 0), "DSFStop2");
	Goto DSFStop2+1;
		
	DSFStop2:
		TNT1 A 0 A_Stop;
		TNT1 A 0 doomb_spawn_bloodpool();
		TNT1 a 0 doomb_spray_decal("BrutalBloodSuper", times: 5);
		TNT1 A 0 doomb_severed_limb("DeadZombiemanShotguntotheFace2", limb_ignore_cvar: true);
	Stop;
		
		
	DeathShotgunFly:
		TNT1 A 0 {bNODROPOFF = false;}
		TNT1 A 0 {bSLIDESONWALLS = false;}
		TNT1 A 0 ThrustThingZ(0,30,0,1);
		TNT1 A 0 A_Scream;
		TNT1 A 0 A_NoBlocking;
		TNT1 A 0 A_FaceTarget;
		TNT1 A 0 A_Recoil (6);
		POS4 UVWXY 5
		{
			doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), doomb_random_velocity(5) );
			if(vel == (0, 0, 0) )
			{
				setstatelabel("TakeASit");
				return;
			}
		}
		TNT1 A 0 doomb_spawn_bloodpool();
		TNT1 A 0 doomb_severed_limb("DeadZombieman1", limb_ignore_cvar: true);
	Stop;
	
	TakeASit:
		POS4 U 5 doomb_spray_decal("BrutalBloodSuper", times: 5);
		TNT1 A 0 doomb_spawn_bloodpool();
		TNT1 A 0 doomb_severed_limb("DeadZombieman_Slumped", limb_ignore_cvar: true);
	Stop;
		
		
	Death:
		TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Death.Down");
		TNT1 AA 0 doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), doomb_random_velocity(5) );
		TNT1 A 0 A_Jump(32, "Death.Arm");
	Death.MinorLeg:
		TNT1 A 0 A_Jump(128, "DeathMirror");
		POSS Q 8 A_Screamandunblock;
		POSS I 5 ;
		POSS JK 5;
		TNT1 A 0 doomb_spawn_bloodpool();
		TNT1 A 0 doomb_severed_limb("DeadZombieman1", limb_ignore_cvar: true);
	Stop;
		
	Death.Rare:
		TNT1 A 0 A_FaceTarget;
		12DG A 5 A_Scream;
		12DG B 5 BRIGHT
		{
			A_PlaySound("weapons/rifle");
			A_SpawnItem ("RifleCaseSpawn", 0, 30,0);
			A_SpawnProjectile ("ZombieManTracer", 24, 5, -20, 2, -20);
		}
		12DG CD 5;
		12DG E 5 BRIGHT
		{
			A_PlaySound("weapons/rifle");
			A_SpawnItem ("RifleCaseSpawn", 0, 30,0);
			A_SpawnProjectile ("ZombieManTracer", 24, -5, 20, 2, 30);
		}
		TNT1 A 0 A_NoBlocking;
		12DG FGHI 5;
		TNT1 A 0 doomb_spawn_bloodpool();
		12DG I 0;
		TNT1 A 0 doomb_severed_limb("DeadZombiemanLikeInActionMovie", limb_ignore_cvar: true);
	Stop;
		
	DeathMirror:
		POSS R 8 A_ScreamandunBlock;
		POSS MNOP 5;
		TNT1 A 0 doomb_spawn_bloodpool();
		POSS P -1 doomb_final_absolution(12, 12);
	Stop;
		
	Death.Arm:
		TNT1 A 0 doomb_severed_limb("XDeathArm1", (0, 0, 35), doomb_random_velocity(5) );
		TNT1 A 0 doomb_much_blood(35);
		TNT1 A 0 A_NoBlocking;
		ZZD1 I 10 A_Scream;
		TNT1 A 0 doomb_severed_limb("DyingZombiemanNoArm", limb_ignore_cvar: true);
	Stop;

	Death.Leg:
		TNT1 A 0 A_JumpIfHealthLower(-300,"DoubleLegRemove");
		TNT1 A 0 A_JumpIfHealthLower(-20, "LegRemove");
		TNT1 A 0 ;
		POSL MO 6;
		POSS K 6;
		TNT1 A 0 doomb_spawn_bloodpool();
		TNT1 A 0 doomb_severed_limb("DeadZombieman1", limb_ignore_cvar: true);
	Stop;
		
	DoubleLegRemove:
		TNT1 A 0 ;
		TNT1 AA 0 doomb_severed_limb("XDeathZombieLeg", (0, 0, 15), doomb_random_velocity(5) );
		TNT1 AA 0 doomb_spawn_entrails( (0, 0, random(30, 35) ) );
		TNT1 A 0 ThrustThingZ(0,50,0,1);
		POS8 ABC 3;
		POS8 D 10;
		TNT1 A 0 doomb_spawn_bloodpool();
		POS8 D -1 doomb_final_absolution(12, 12);
	Stop;
		
	LegRemove:
		TNT1 A 0 A_Jump(64, "LegRemoveRare");
		TNT1 A 0 doomb_much_blood(15);
		TNT1 AA 0 doomb_spawn_entrails( (0, 0, random(30, 35) ) );
		TNT1 AA 0 doomb_severed_limb("XDeathZombieLeg", (0, 0, 35), doomb_random_velocity(5) );
		TNT1 A 0 doomb_spawn_bloodpool();
		TNT1 A 0 A_Screamandunblock;
		TNT1 A 0 doomb_severed_limb("ZombiemanGotHisLegBlowed", limb_ignore_cvar: true);
	Stop;
		
	LegRemoveRare:
		TNT1 A 0 doomb_much_blood(15);
		TNT1 AA 0 doomb_spawn_entrails( (0, 0, random(30, 35) ) );
		TNT1 AA 0 doomb_severed_limb("XDeathZombieLeg", (0, 0, 35), doomb_random_velocity(5) );
		TNT1 A 0 doomb_spawn_bloodpool();
		TNT1 A 0 A_Screamandunblock;
		TNT1 A 0 doomb_severed_limb("ZombiemanSacilol", limb_ignore_cvar: true);
	Stop;
		
		
		
	Death.Saw:
		TNT1 A 0 A_Jump(96, "Death.arm");
	Goto Death.Tear;
		
	Death.cut:
	Death.HKFT:
		TNT1 A 0 doomb_much_blood(35);
		TNT1 A 0 A_Screamandunblock;
		TNT1 AAA 0 doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), doomb_random_velocity(5) );
		TNT1 A 0 doomb_severed_limb ("XDeathHalfZombieMan", (0, 0, 32), doomb_random_velocity(5) );
		TNT1 AA 0 doomb_spawn_entrails( (0, 0, random(30, 35) ) );
		TNT1 AAAAA 0 doomb_severed_limb("Intestine", (0, 0, 32), (frandom(-10, 10), frandom(-10, 10), frandom(0, 10) ) );
		TNT1 AAAA 0 doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), doomb_random_velocity(5) );
		POSX AABCDE 9 doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), doomb_random_velocity(5) );
		TNT1 A 0 doomb_spawn_bloodpool();
		POSX EEEEEEEEEEEEEEEEEEEEEE 1 doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), doomb_random_velocity(5) );
		POSX E 0;
		POSX E -1 doomb_final_absolution(12, 12);
	Stop;
	
	Death.cutless:
		TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Death.Down");
		TNT1 A 0 A_Jump(64, "Death.Rare");
		TNT1 A 0 A_Jump(16, "Death.Saw");
		TNT1 A 0 A_Jump(9, "Death.LegMassacre");
		TNT1 A 0 A_Jump(64, "Death");
	Goto Death.Minigun;
		
	Death.Minigun:
		TNT1 A 0 A_Scream;
		TNT1 O 0 A_NoBlocking;
		TNT1 A 0 doomb_severed_limb("BrutalizedZombieMan1", limb_ignore_cvar: true);
	Stop;
		
	Death.LegMassacre:
		TNT1 A 0 doomb_much_blood(15);
		TNT1 A 0 doomb_severed_limb("BrutalizedZombieManLeg");
		POS7 B 1 A_Scream;
		TNT1 A 0 A_NoBlocking;
		TNT1 A 0 A_PlaySound("misc/xdeath2");
		LEG1 IJK 8;
		LEG1 E 1 doomb_spawn_bloodpool(BPS_SMALL);
		LEG1 E -1 doomb_final_absolution(12, 12);
	Stop;
		
	Death.Tear:
		TNT1 A 0 doomb_much_blood(1);
		TNT1 A 0 A_XScream;
		TNT1 O 0 A_NoBlocking;
		PP2X AA 8 doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), doomb_random_velocity(5) );
		TNT1 AA 0 doomb_spawn_entrails( (0, 0, random(30, 35) ) );
		tnt1 a 0 A_Explode(3,200, 0, 0, 0, 0, 0, "none", "blood");
		TNT1 AAA 0 doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), doomb_random_velocity(5) );
		TNT1 AAAAAA 0 doomb_severed_limb("Intestine", (0, 0, 32), (frandom(-10, 10), frandom(-10, 10), frandom(0, 10) ) );
		TNT1 A 0 doomb_severed_limb("XDeathHalfZombieman2", (0, 0, 32), doomb_random_velocity(5) );
		PP2X BBCD 8 doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), doomb_random_velocity(5) );
		TNT1 A 0 doomb_spawn_bloodpool();
		PP2X EEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEE 1 doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), doomb_random_velocity(5) );
		PP2X E -1 doomb_final_absolution(12, 12);
	Stop;
		
		
		
		
	Death.Eat:
	Death.BHFT:
	Death.RVFT:
		TNT1 A 1;
	Stop;
		
	Death.Headshot:
		TNT1 A 0 A_Jump(126, "headshot_1");
		TNT1 A 0 A_JumpIfHealthLower(-600, "XDeath");
		TNT1 A 0 A_JumpIfHealthLower(-250, "Death.SSGHead");
		TNT1 A 0 A_JumpIfHealthLower(-50, "Death.HeadExplode");
		TNT1 A 0 A_Jump (96, "Death_headshot_1", "Death_headshot_2");
		TNT1 A 0 A_Scream;
		TNT1 A 0 doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), doomb_random_velocity(5) );
		TNT1 A 0 doomb_much_blood(40);
		POSH V 5;
		TNT1 AAAA 0 doomb_severed_limb("SmallBrainPiece", (0, 0, random (45, 55) ), doomb_random_velocity(5) );
		TNT1 a 0 doomb_spray_decal("BrutalBloodSuper", times: 5);
		POSH W 5 A_NoBlocking;
		POSH XY 5;
		TNT1 A 0 doomb_spawn_bloodpool();
		POSH Z -1 doomb_final_absolution(12, 12);
	Stop;

	Death_headshot_1:
		TNT1 A 0 A_NoBlocking;
		TNT1 A 0 A_SpawnItem("DyingZombiemanLostTeeth");
		Stop;
		
		
	Death_headshot_2:
		TNT1 A 0 A_Pain;
