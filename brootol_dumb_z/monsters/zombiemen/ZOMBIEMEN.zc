CLASS ZombiemanGrenade : doomb_boolet_base { 
Default
{
	Radius 4;
	Height 4;
	Speed 20;
	Damage 20;
	Gravity 1.0;
	Scale 0.2;
	Speed 15;
	Damage 0;
	BounceFactor 0.7;

	+MISSILE;
	+BLOODSPLATTER;
	+EXTREMEDEATH;
	+DOOMBOUNCE;
	+GHOST;
	+THRUGHOST;

	Decal "Scorch";
	SeeSound "none";
	DeathSound "Explosion";
	Obituary "$OB_MPROCKET";
	Damagetype "Explosive_Impact";
}

private uint timer;

States
{
	Spawn:
		TNT1 A 0 nodelay {vel.z += 10;}
	Live:
		GRND ABCDEFGH 2
		{
			vel.z -= 1;
			timer ++;
			if(timer > 20)
				setstatelabel("death");
		}
	Loop;
		
		
	Death:
	explosion:
		TNT1 a 1
		{
			Radius_Quake (2, 8, 0, 15, 0);//(intensity, duration, damrad, tremrad, tid)
			if(waterlevel > 1)
				spawn("UnderwaterExplosion", pos:self.pos);
			
			{doomb_flare_base.doomb_attach_flare(self, "NewfagExplosionFlare");}
			doomb_show_effect("HeavyExplosionSmoke", (0, 0, 0), (frandom(-1, 1), frandom(-1, 1), frandom(0.5, 1) ) );
			doomb_show_effect("ExplosionSmoke", (0, 0, 0), (frandom(-1, 1), frandom(-1, 1), frandom(-0.5, 1) ) );
			doomb_show_effect("ExplosionFlames", (0, 0, 0), (frandom(-1, 1), frandom(-1, 1), frandom(-1, 1) ), (frandom(0.9, 1.4), 0) );
			doomb_show_effect("ExplosionFlames", (0, 0, 0), (frandom(-1, 1), frandom(-1, 1), frandom(-1, 1) ), (frandom(0.9, 1.4), 0) );
			A_Explode(20, 70, 1);
			A_PlaySound("FAREXPL", 3);
		
			for(uint i = 0; i < 30; i++)
				doomb_show_effect("ExplosionParticle2", (0, 0, 0), (frandom(-1, 1), frandom(-1, 1), frandom(0.5, 1) ) );
		}
	Stop;
}

