

CLASS XDeathCyberdemonHead: XDeathBullArm1 { 
	Default {
		
		-CLIENTSIDEONLY;
		+SKYEXPLODE;
		-DOOMBOUNCE;
		Scale 1.2;
	}
	States
	{
		Spawn:
			TNT1 A 0 A_SpawnItem ("Brutal_BloodSpot", 0, 1);
			TNT1 A 0 A_JumpIf(waterlevel > 1, "Death");
			TNT1 A 0 A_JumpIf(vel.z == 0, "Death");
			XHE8 ABCDEFGH 3 A_SpawnProjectile ("Brutal_FlyingBlood", 1, 0, random (0, 360), 2, random (20, 40));
			Loop;
		Death:
			TNT1 A 0 A_SpawnItem ("Brutal_BloodSpot", 0, 1);
			XHE8 B 1;
			XHE8 B 1 A_SpawnItem("CyberdemonHead",0,0,0,1);
			Stop;
	}
}


CLASS CyberdemonHead: LimbBase { 
	Default {
		
		-CLIENTSIDEONLY;
		Radius 20;
		Mass 220;
		Health 100;
		Scale 1.2;
	}
	States
	{
		Spawn:
			TNT1 A 0;
			TNT1 A 0 A_SpawnItem ("Brutal_BloodSpot", 0, 1);
			XHE8 C 1;
			XHE8 C -1;
			Loop;
			
		Death:
		Death.Trample:
		Crush:
			TNT1 A 0 A_PlaySound("misc/xdeath4");
			
			TNT1 AAAAAAAAA 0 doomb_for_khorne("blood_impact_splash");
			TNT1 AAAAA 0 A_SpawnProjectile ("Brutal_FlyingBlood", 0, 0, random (0, 360), 2, random (30, 90));
			TNT1 AAA 0 A_SpawnProjectile ("head_piece_dark", 10, 0, random (0, 360), 2, random (0, 160));
			TNT1 AAA 0 A_SpawnProjectile ("jaw_big", 10, 0, random (0, 360), 2, random (0, 160));
			TNT1 AAA 0 A_SpawnProjectile ("XDeath1", 10, 0, random (0, 360), 2, random (0, 160));
			TNT1 AAAAA 0 A_SpawnProjectile ("XDeath3", 10, 0, random (0, 360), 2, random (50, 90));
			TNT1 AAAAA 0 A_SpawnProjectile ("XDeath2", 10, 0, random (0, 360), 2, random (50, 90));
			TNT1 A 0 A_Stop;
			XHE8 I 10;
			XHE8 I -1;
			Stop;
			
		Death.Explosive_Impact:
			TNT1 A 0 A_NoBlocking;
			TNT1 A 0 ThrustThingZ(0,30,0,1);
			XHE8 ABCDEFGH 3;
			XHE8 B 0 A_SpawnItem("CyberdemonHead");
			Stop;
			
	}
	
}


