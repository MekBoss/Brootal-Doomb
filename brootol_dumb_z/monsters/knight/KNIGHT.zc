			TNT1 A 0 ThrustThingZ(0,5,0,1);
			TNT1 A 0 {bNOPAIN = true;}
			KNRJ ABCD 5;
			TNT1 A 0 A_Pain;
			KNRJ ABCD 5;
			TNT1 A 0 A_Pain;
			KNRJ ABCD 5;
			TNT1 A 0 A_Pain;
			KNRJ ABCD 5;
		Goto Missile;
			
