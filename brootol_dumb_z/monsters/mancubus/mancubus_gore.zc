CLASS XDeathdoomb_fatsoArm: XDeathArm1 { 
	Default {
 
    +SKYEXPLODE
	-CLIENTSIDEONLY
    Scale 1.1;
    Speed 9;
	Gravity 0.4;
	bounceFactor 0.4;
	}
	States
	{
		Spawn:
			TNT1 A 0 A_JumpIf(waterlevel > 1, "Death");
			MANA ABCDEFG 3;
			Loop;
		Death:
			TNT1 A 0 A_SpawnItem ("Brutal_BloodSpot", 0, 1);
			HND8 A 0;
			HND8 A 0 A_SpawnItem("FlameCannon",0,0,0,1);
			Stop;
	}
}
