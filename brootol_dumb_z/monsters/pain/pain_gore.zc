	Scale 1.1;
	}
    States
    {
	Spawn:
		TNT1 A 0 A_JumpIf(waterlevel > 1, "Splash");
		CAGI ABCDEFGH 2 doomb_for_khorne("doomb_red_blood_trail", (0, 0, 0 ), (frandom(-5, 5), frandom(-5, 5), frandom(0, 5) ) );
	Loop;

	Death:
		
		TNT1 A 0;
		TNT1 AAA 0 A_SpawnItem("Blood", 0, 5);
		TNT1 A 0 A_CheckFloor("SpawnFloor");
		TNT1 A 0 A_SpawnItemEx ("CeilBloodLauncher",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0);
		TNT1 A 0 doomb_meat_on_ceiling();
		CAGI Q 9009;
		TNT1 A 2;
	Stop;

	SpawnFloor:
		CAGI Q 9009;
		TNT1 A 2;
	Stop;
}
}



