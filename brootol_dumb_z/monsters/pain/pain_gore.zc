
CLASS PEPart1 : doomb_body_base { 
	Default {
		
		Radius 8;
		Height 8;
		Speed 12;
		Scale 1.5;
		Mass 1;
		+NOBLOCKMAP;
		+MISSILE;
		+NOTELEPORT;
		+MOVEWITHSECTOR;
		+CLIENTSIDEONLY;
		+RIPPER;
		+BLOODLESSIMPACT;
		+DOOMBOUNCE;
		+SKYEXPLODE;
		BounceFactor 0.5;
		damagetype "Blood";
		SeeSound "misc/xdeath4";
		DeathSound "misc/xdeath1";
		
	}
	States
	{
		Spawn:
			TNT1 A 0 A_JumpIf(waterlevel > 1, "Splash");
			ID22 ABCD 2
			{
				doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), (frandom(-5, 5), frandom(-5, 5), frandom(0, 5) ) );
				doomb_for_khorne("doomb_red_blood_trail", (0, 0, random(20, 30) ), (frandom(-5, 5), frandom(-5, 5), frandom(0, 5) ) );
				doomb_show_effect("FlameTrails", ( 0, 0, 0), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(0.5, 1) ) );	
			}
			Loop;
		Death:
			ID22 F 1;
			ID22 F -1;
			Stop;
			TNT1 A 1;
		Splash:
			TNT1 A 0;
			Stop;
	}
}


