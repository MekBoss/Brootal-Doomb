CLASS BurningBarrel1 : ACTOR replaces BurningBarrel { 
	Default {
		
		+LOOKALLAROUND;
		+SHOOTABLE;
		+NOBLOOD;
		+GHOST;
		+NOTARGET;
		+FRIENDLY;
		-BOSS;
		+RANDOMIZE;
		Radius 16;
		+SOLID;
		height 40;
		Health 30;
		DamageFactor "kick", 5.0;
		DamageFactor "SuperPunches", 5.0;
		Damagefactor "SuperKick", 5.0;
		Damagefactor "punch", 5.0;
	}
	states
	{
		Vanilla:
			TNT1 A 0 A_NoBlocking();
			TNT1 A 0 A_SpawnItem("BurningBarrelVanilla");
			Stop;
			
		Spawn:
			TTRB A 0;
			TNT1 A 0 A_Playsound("BDFireplace", 1, 1, 1);
			MARN A 0 ACS_ExecuteAlways(795, 0, 0, 0, 0);//Check if advanced decorations are disabled.
			TNT1 A 1;
			
		Stay:
			TNT1 A 0 A_CheckSight("Spawn2");
			TNT1 A 0 bright A_SpawnProjectile ("MediumBlackSmoke", 38, 0, random (40, 180), 2, - random (60, 120));
			TNT1 A 0 bright A_SpawnProjectile ("RealFlameTrailsSmallLong", 38, 0, random (40, 180), 2, - random (50, 130));
			TNT1 A 0 A_SpawnProjectile ("ImBallGettingReady2", 35, 0, random (0, 140), 2, - random (0, 160));
			BURB AAA 1 bright A_SpawnItem("RedFlare3",0,42);
			
			Loop;
			
		Spawn2:
			BURB A 10;
			Goto Stay;
			
		Pain.Kick:
		Death.punch:
		Death.Kick:
		Death.SuperKick:
		Death.SuperPunches:
			TNT1 A 0 A_Pain();
			TNT1 A 0 A_StopSound(1);
			TNT1 A 1 A_FaceTarget();
			TNT1 A 0 A_NoBlocking();
			TNT1 AAA 0 A_SpawnProjectile ("FlyingBurningFuel", 24, 0, random (150, 210), 2, - random (0, 40));
			TNT1 AA 0 A_SpawnProjectile ("FlyingBurningFuel2", 24, 0, random (150, 210), 2, - random (0, 40));
			TNT1 AA 0 A_SpawnProjectile ("FlyingBurningFuel3", 24, 0, random (150, 210), 2, - random (0, 40));
			BARK DE 3;
			BARK F -1;
			Stop;
			
		Death.Explosive_Impact:
			TNT1 A 0 A_NoBlocking();
			TNT1 A 0 A_StopSound(1);
			TNT1 AAA 0 A_SpawnProjectile ("FlyingBurningFuel", 24, 0, random (0, 360), 2, - random (30, 80));
			TNT1 AA 0 A_SpawnProjectile ("FlyingBurningFuel2", 24, 0, random (0, 360), 2, - random (30, 80));
			TNT1 AA 0 A_SpawnProjectile ("FlyingBurningFuel3", 24, 0, random (0, 360), 2,  - random (30, 80));
			TNT1 A 0 A_SpawnItem ("BigRicoChet", 0, -15);
			TNT1 A 0 A_SpawnItemEx ("DetectFloorCrater",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0);
			TNT1 A 0 A_SpawnItemEx ("DetectCeilCrater",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0);
			TNT1 A 0 A_SpawnItemEx ("ExplosionFlareSpawner",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0);
			TNT1 A 0 A_Explode(160,200);
			TNT1 AAAAAA 0 A_SpawnProjectile ("SmallExplosionSpawner", 20, 0, random (0, 360), 2, - random (0, 180));
			TNT1 AAAAAAA 0 A_SpawnProjectile ("ExplosionFlames", 0, 0, random (0, 360), 2, random (0, 360));
			TNT1 AAAAAAAAAAAAAAAAAAAAAAAAAA 0 A_SpawnProjectile ("ExplosionParticle2", 0, 0, random (0, 360), 2, random (0, 180));
			TNT1 AAAAAA 0 A_SpawnProjectile ("ExplosionParticle2", 0, 0, random (0, 360), 2, random (0, 180));
			TNT1 AAA 0 A_SpawnProjectile ("BarrelShrapnel1", 25, 0, random (0, 360), 2, - random (30, 160));
			TNT1 AAA 0 A_SpawnProjectile ("BarrelShrapnel2", 15, 0, random (0, 360), 2, - random (30, 160));
			TNT1 AAA 0 A_SpawnProjectile ("BarrelShrapnel3", 5, 0, random (0, 360), 2, - random (30, 160));
			TNT1 AAA 0 A_SpawnProjectile ("BarrelShrapnel4", 5, 0, random (0, 360), 2, - random (30, 160));
			tnt1 a 0
			{
				a_setscale(3.0);
				a_setrenderstyle(0.12, STYLE_add);
				
			}
			SHWK ACEGIKMOQ 1 A_FadeOut(0.01);
		Stop;
		
		
	}
}


