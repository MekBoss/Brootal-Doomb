CLASS TallTorch_Red : RedTorch replaces RedTorch { 
	Default {
		
		height 85;
		radius 10;
		+LOOKALLAROUND;
		-SHOOTABLE;
		+INVULNERABLE;
		+NOBLOOD;
		+GHOST;
		+RANDOMIZE;
		Mass 900000;
		scale .5;
	}
	states
	{
		Vanilla:
			TNT1 A 0 A_NoBlocking();
			TNT1 A 0 A_SpawnItem("RedTorchVanilla");
			Stop;
			
		Spawn:
			TTRB A 0;
			TNT1 A 0 A_Playsound("BDFireplace2", 1, 1, 1);
			MARN A 0 ACS_ExecuteAlways(795, 0, 0, 0, 0);//Check if advanced decorations are disabled.
			TNT1 A 1;
			
		Stay:
			TNT1 A 0 A_CheckSight("Spawn2");
			TNT1 A 0 bright A_SpawnProjectile ("RealFlameTrailsSmall", 58, 0, random (0, 360), 2, - random (70, 110));
			TNT1 A 0 A_SpawnProjectile ("ImBallGettingReady3", 59, 0, random (0, 140), 2, - random (0, 160));
			TTRB AC 2 A_SpawnItem("RedFlare22",0,59);
			Loop;
			
		Spawn2:
			TTRB A 6;
			Goto Stay;
			
	}
}

