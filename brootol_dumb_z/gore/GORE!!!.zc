CLASS BigGibBase : doomb_body_base { 
	Default {
		Radius 10;
		Height 10;
		+CLIENTSIDEONLY;
		+NOTELEPORT;
		+MOVEWITHSECTOR;
		+DONTSPLASH;
		+NOBLOCKMONST;
		-NOBLOCKMAP;
		+GHOST;
		+THRUACTORS;
		+FLOORCLIP;
		+SLIDESONWALLS;
		+FORCEXYBILLBOARD;
		DamageFactor "CauseObjectsToSplash", 0.0;
		DamageFactor "GibRemoving", 0.5;
		-SOLID;
		+SHOOTABLE;
		damagefactor "Crush", 50.0;
		damagefactor "Plasma", 20.0;
		damagefactor "TeleportRemover", 1000.0;
		painchance "Flames", 0;
		painchance "Fire", 0;
		painchance "Burn", 0;
		painchance "KillMe", 0;
		damagefactor "Kick", 3.0;
		damagefactor "SuperKick", 3.0;
		damagefactor "punch", 2.0;
		Mass 1000;
		Health 100;
		damagefactor "Blood", 0.0;
		damagefactor "GreenBlood", 0.0;
		damagefactor "BlueBlood", 0.0;
		damagefactor "Taunt", 0.0 ;
		damagefactor "KillMe", 0.0 ;
		damagefactor "Avoid", 0.0;
		damagefactor "Taunt", 0.0;
		damagefactor "Trample", 5000.0;
		painchance 255;
	}
	States 
	{
		Spawn:
			TNT1 A 1900 A_SpawnItem ("Brutal_BloodSpot", 0, 1);
			Loop;
			
			
		Death:
			TNT1 AAA 0 A_SpawnProjectile ("XDeath1", 5, 0, random (0, 360), 2, random (0, 160));
			TNT1 AAAAAAA 0 A_SpawnProjectile ("XDeathSkinPiece2", 5, 0, random (0, 360), 2, random (0, 160));
			TNT1 AAAA 0 A_SpawnProjectile ("XDeathSkinPiece1", 5, 0, random (0, 360), 2, random (0, 160));
			TNT1 AAAA 0 A_SpawnProjectile ("SmallSuperGore", 10, 0, random (0, 360), 2, random (10, 170));
			Stop;
			
		Death.plasma:
		Death.Fire:
			TNT1 A 0;
			TNT1 A 0 A_NoBlocking;
			TNT1 A 0 A_Jump(128, "DeathFire2");
			TNT1 A 0 doomb_spawn_burned_flesh();
			EXPL AAA 0 doomb_show_effect("ExplosionSmoke", (0, 0, 0), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(-0.5, 1) ) );
			TNT1 A 0;
			Stop;
			
		DeathFire2:
			TNT1 A 0 doomb_spawn_burned_flesh();
			EXPL AAA 0 doomb_show_effect("ExplosionSmoke", (0, 0, 0), (frandom(-0.5, 0.5), frandom(-0.5, 0.5), frandom(-0.5, 1) ) );
			TNT1 A 0;
			Stop;
		Death.Trample:
		Death.Crush:
		Death.Stomp:
		Death.SuperPunch:
		Death.punch:
			TNT1 AAA 0 A_SpawnProjectile ("XDeath1", 5, 0, random (0, 360), 2, random (0, 30));
			TNT1 AAAA 0 A_SpawnProjectile ("BloodMist", 5, 0, random (0, 360), 2, random (0, 30));
			TNT1 AAAAAAA 0 A_SpawnProjectile ("XDeathSkinPiece2", 5, 0, random (0, 360), 2, random (10, 30));
			TNT1 AAAA 0 A_SpawnProjectile ("XDeathSkinPiece1", 5, 0, random (0, 360), 2, random (10, 130));
			TNT1 AAAA 0 A_SpawnProjectile ("SmallSuperGore", 10, 0, random (0, 360), 2, random (10, 30));
			TNT1 A 0 doomb_spawn_bloodpool();
			XME2 L 1;
			XME2 L -1;
			Stop;
		Death.TeleportRemover:
			TNT1 A 0;
			TNT1 A 0 {bNOCLIP = true;}
			TNT1 A 0 {bSHOOTABLE = false;}
			TNT1 A 0 A_NoBlocking;
			Stop;
		Death.GibRemoving:
			TNT1 A 0;
			TNT1 A 0 {bNOCLIP = true;}
			TNT1 A 0 {bSHOOTABLE = false;}
			TNT1 A 0 A_NoBlocking;
			Stop;
			
		Death.Desintegrate:
			TNT1 A 0;
			TNT1 AAAA 0 A_SpawnProjectile ("Brutal_FlyingBloodFake", 0, 0, random (0, 360), 2, random (0, 90));
			TNT1 A 0 A_SpawnProjectile ("Brutal_FlyingBlood", 7, 0, random (0, 360), 2, random (30, 60));
			TNT1 A 0 A_SpawnProjectile ("BloodMistSmall", 7, 0, random (0, 360), 2, random (30, 60));
			Stop;
			
	}
	
}

