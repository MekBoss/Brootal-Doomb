CLASS XDeath1 : doomb_body_base { 
	Default {
 
    Radius 8;
    Height 8;
    Speed 10;
    Scale 1.2;
	Mass 1;
    +NOBLOCKMAP
	+MISSILE
    +NOTELEPORT
    +MOVEWITHSECTOR
    +CLIENTSIDEONLY
    +BLOODLESSIMPACT 
	+DONTSPLASH
	+THRUGHOST
	+thrUACTORS
		+FLOORCLIP;
		damagetype "Blood";
		SeeSound "misc/xdeath4";
		DeathSound "misc/xdeath1";
		Decal "BrutalBloodSuper";
		Mass 1;
		Gravity 0.4;
	}
	States
	{
		Spawn:
			TNT1 A 0 A_JumpIf(waterlevel > 1, "Underwater");
			BLOD A 4 A_SpawnProjectile ("Brutal_LiquidBloodTrail", 0, 0, random (0, 360), 2, random (0, 360));
			Loop;
		Death:
			XDT1 EFGHIJKL 5;
			TNT1 A 0 A_SpawnItem ("Brutal_BloodSpot", 0, 1);
			Stop;
			TNT1 A 1;
		Underwater:
		Splash:
			XDT1 ABCD 3
			{
				doomb_for_khorne("Brutal_FlyingBloodTrail", (0, 0, 0 ), (frandom(-5, 5), frandom(-5, 5), frandom(0, 5) ) );
				vel.z += 3;
			}
		stop;
	}
}

CLASS XDeath2: XDeath1 { 
	Default {
		DeathSound "misc/xdeath2";
	}
	States
	{
		Spawn:
			TNT1 A 0 A_JumpIf(waterlevel > 1, "Splash");
			XME1 ABCD 3 doomb_for_khorne("Brutal_FlyingBloodTrail", (0, 0, 0 ), (frandom(-5, 5), frandom(-5, 5), frandom(0, 5) ) );
			Loop;
		Death:
			TNT1 AAAAAAA 0 A_SpawnProjectile ("Brutal_FlyingBloodTrail", 0, 0, random (0, 180), 2, random (0, 180));
			TNT1 A 0 A_CheckFloor("SpawnFloor");
			TNT1 A 0 A_SpawnItemEx ("CeilBloodLauncher",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0);
			TNT1 A 0 doomb_meat_on_ceiling();
			Stop;
			
		SpawnFloor:
			XME1 E 1;
			XME1 E 3;
			TNT1 A 0 A_SpawnItem ("Brutal_BloodSpot", 0, 1);
		Living:
			XME1 E 600;
			Loop;
		Vanish:
			TNT1 A 5;
			Stop;
	}
}


