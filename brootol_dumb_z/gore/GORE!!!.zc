CLASS Gib_Arm: LimbBase { 
	Default {
		
		Scale 1.1;
	}
	States
	{
		Spawn:
			XDT5 I 1900 A_SpawnItem ("Brutal_BloodSpot", 0, 1);
			Loop;
		Death.plasma:
		Death.fire:
			TNT1 A 0 A_SpawnProjectile ("XDeathBurnedArm", 32, 0, random (0, 360), 2, random (0, 160));
			Stop;
		Death:    
			TNT1 AAAA 0 doomb_for_khorne("Brutal_FlyingBloodTrail", (0, 0, 0 ), (frandom(-5, 5), frandom(-5, 5), frandom(0, 5) ) );
			TNT1 A 0 A_SpawnProjectile ("GibHand", 0, 0, random (0, 180), 2, random (0, 180));
			TNT1 A 0 A_SpawnProjectile ("GibArmPart", 0, 0, random (0, 180), 2, random (0, 180));
			Stop;
			
		Death.Kick:
		Death.punch:
			XDT5 I 1 A_FaceTarget;
			TNT1 A 0 A_SpawnProjectile ("XDeathArm1", 5, 0, random (170, 190), 2, random (20, 40));
			Stop;
		Pain:
			TNT1 A 0;
			TNT1 A 0 ThrustThingZ(0,20,0,1);
			XARM ABCD 3;
			Goto Spawn;
			
		Death.Explosive_Impact:
			TNT1 A 0;
			TNT1 A 0 ThrustThingZ(0,30,0,1);
			XARM A 6;
			XARM ABCDABCDABCD 6 A_CheckFloor("Landz");
		Landz:
			XDT5 I 0 A_SpawnItem("Gib_Arm");
			Stop;
	}
}

