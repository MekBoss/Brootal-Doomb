addkeysection "Brootal Doomb Zscript Actions" [brootal_doomb]

addmenukey "Kick" doomb_wp_kc_tt
defaultbind q "doomb_wp_kc_tt"
alias doomb_wp_kc_tt "netevent do_kick"

addmenukey "Grenade throw" doomb_gr_tt
defaultbind f "doomb_gr_tt"
alias doomb_gr_tt "netevent throw_grenade"

addmenukey "Dual wielding" doomb_wp_dl_tt
defaultbind  v "doomb_wp_dl_tt"
alias doomb_wp_dl_tt "netevent weapon_dual"

addmenukey "Taunt" doomb_taunt
alias doomb_taunt "netevent taunt"
defaultbind H doomb_taunt

addmenukey "Salute" doomb_salute
alias doomb_salute "netevent salute"
defaultbind V doomb_salute


addmenukey "Roll Left" doomb_roll_left
alias doomb_roll_left "netevent roll_left"
defaultbind Z doomb_roll_left


addmenukey "Roll Right" doomb_roll_right
alias doomb_roll_right "netevent roll_right"
defaultbind C doomb_roll_right
